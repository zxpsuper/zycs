<view class="support-name">
  <text>{{data.name}}</text>
  <view class="donate">
    <text>捐赠金额：<text class="donateMoney">{{payMoney}}</text>元</text>
  </view>
</view>
<view class="pay-success">
  <image src="{{imageName}}" class="image"></image>
  <view class="successText">
    <text>{{payResult}}</text>
  </view>
</view>
<view class="btn" bindtap="discover">
  <text>查看更多项目</text>
</view>