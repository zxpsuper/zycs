.upload-btn-wrapper {
  float: left;
  position: relative;
  min-height: 1px;
  padding-left: 5px;
  padding-right: 5px;
  width: 33.33333%;
}

.upload-btn, ul.uploader-list .upload-item {
  margin: 5px 0;
  width: 100%;
  padding-bottom: 100%;
  background-color: #eee;
  overflow: hidden;
}

.upload-item {
  position: relative;
  color: inherit;
  background-color: #eee;
  border-color: #eee;
}

.upload-btn .img-tips, ul.uploader-list .upload-item .img-tips {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 90%;
  transform: translate(-50%, -50%);
}

.upload-item .img-tips {
  text-align: center;
  margin: 0 auto;
  display: block;
}

.upload-item .img-tips img {
  width: 24px;
  height: 20px;
  display: block;
  max-width: 100%;
  margin-left: auto;
  margin-right: auto;
}

.upload-btn .img-tips text {
  padding-top: 5px;
  color: #aaa;
  font-size: 0.75rem;
  margin-top: 5px;
}

