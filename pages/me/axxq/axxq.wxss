html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video, view,navigator {
    margin: 0;
    padding: 0;
    border: 0;
    vertical-align: baseline;
}
.html {
    font-size: 100%;
    font-family: "Microsoft YaHei",Helvetica,Arial,sans-serif;
    -ms-text-size-adjust: 100%;
    -webkit-text-size-adjust: 100%;
    line-height: 1.6;
    -webkit-tap-highlight-color: transparent;
}
.fnav-wrapper {
    padding-bottom: 3.25rem;
}
.body {
    background-color: #f9f9f9;
    color: #666;
    margin: 0 auto;
    font-size: 0.85rem;
    word-wrap: break-word;
    max-width: 640px;
    min-height: 100%;
    overflow-x: hidden;
}
.order-detail {
    margin: 1.25rem 0.625rem;
}
.order-detail .order-decor {
    background-color: #aaa;
    width: 100%;
    height: 0.75rem;
    -webkit-border-radius: 0.75rem;
    -moz-border-radius: 0.75rem;
    -ms-border-radius: 0.75rem;
    -o-border-radius: 0.75rem;
    border-radius: 0.75rem;
}
.order-detail .order-main {
    overflow: hidden;
    padding: 0 0.625rem 0.625rem 0.625rem;
    margin-top: -0.375rem;
}
.order-detail .o-wrap {
    background-color: #fff;
    border-left: 1px solid #eee;
    border-right: 1px solid #eee;
    padding: 0.625rem;
    position: relative;
    -webkit-animation: fadeInDown 0.8s linear 0s 1 normal;
    -moz-animation: fadeInDown 0.8s linear 0s 1 normal;
    -ms-animation: fadeInDown 0.8s linear 0s 1 normal;
    -o-animation: fadeInDown 0.8s linear 0s 1 normal;
    animation: fadeInDown 0.8s linear 0s 1 normal;
    -webkit-animation-fill-mode: forwards;
    -moz-animation-fill-mode: forwards;
    -ms-animation-fill-mode: forwards;
    -o-animation-fill-mode: forwards;
    animation-fill-mode: forwards;
}
.o-wrap {
    padding-left: 0.625rem;
    padding-right: 0.625rem;
}
.o-wrap .o-content {
    padding-bottom: 0.625rem;
}
.group-inline.all-text {
    color: #aaa;
    line-height: 1.4;
}
.group-inline {
    display: table;
    width: 100%;
}
ul, li {
    list-style-type: none;
}
ol, ul {
    list-style: none;
}
.group-inline .group-row {
    display: table-row;
}
.group-inline.all-text .group-cell:first-child {
    text-align: right;
}
.group-inline .group-row:first-child .group-cell {
    border-top: none;
}
.group-inline.all-text .group-cell {
    border-top: none;
    padding: .15rem .2rem;
}
.group-inline.vtop .group-cell {
    vertical-align: top;
}
.group-inline .group-row .group-cell {
    padding-left: 0.625rem;
    padding-right: 0.625rem;
    border-top: 1px solid #eee;
}
.group-inline .group-cell {
    display: table-cell;
    vertical-align: middle;
}
.group-inline .group-row:first-child .group-cell {
    border-top: none;
}
.group-inline.all-text .group-cell {
    border-top: none;
    padding: .15rem .2rem;
}
.group-inline.vtop .group-cell {
    vertical-align: top;
}
.group-inline .group-row .group-cell {
    padding-left: 0.625rem;
    padding-right: 0.625rem;
    border-top: 1px solid #eee;
}
.group-inline .group-cell {
    display: table-cell;
    vertical-align: middle;
}
text{
    font-size: 26rpx;
}
.o-wrap .o-content navigator {
    border-bottom: 1px solid #eceeef;
}
navigator {
    text-decoration: none;
}
navigator, navigator:active, input, button, select, textarea {
    outline: 0px none;
    font-family: inherit;
    -webkit-tap-highlight-color: transparent;
    -webkit-appearance: none;
}
navigator {
    color: inherit;
    cursor: pointer;
    background-color: transparent;
}